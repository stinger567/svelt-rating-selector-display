{
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": true,
	"quoteProps": "consistent",
	"trailingComma": "all",
	"semi": true,
	"arrowParens": "always",
	"printWidth": 150,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
