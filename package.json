{
  "name": "svelt-rating-selector-display",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "exports": {
    "./package.json": "./package.json",
    "./RatingSelector.svelte": {
      "types": "./dist/RatingSelector.svelte.d.ts",
      "svelte": "./dist/RatingSelector.svelte",
      "default": "./dist/RatingSelector.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.1.9",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/stinger567/svelt-rating-selector-display",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stinger567/svelt-rating-selector-display.git"
  },
  "bugs": {
    "url": "https://github.com/stinger567/svelt-rating-selector-display/issues"
  },
  "author": "Stinger567",
  "keywords": [
    "rating",
    "star selector",
    "rating selector",
    "rating dispaly",
    "rating-picker",
    "star-picker",
    "svelte",
    "sveltkit",
    "front-end"
  ],
  "prettier": {
    "useTabs": true,
    "tabWidth": 2,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "all",
    "semi": true,
    "arrowParens": "always",
    "printWidth": 150,
    "pluginSearchDirs": [
      "."
    ]
  },
  "description": "This is a small svelt app that lets you easily get ratings/reviews from user and display them. It uses unicode characters and text as icons."
}
